@using PruebasGemini.Web.Models
@model ExamenModel

@{
    ViewData["Title"] = "MostrarFeedback";
}

<h2>Feedback de las Respuestas</h2>

@if (!string.IsNullOrEmpty(Model.Feedback))
{
    <pre>@Model.Feedback</pre>
}
else
{
    <p>No se pudo obtener el feedback.</p>
}

@if (ViewBag.Error != null)
{
    <h3>Error:</h3>
    <p style="color:red">@ViewBag.Error</p>
}
